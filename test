width = 200
height = 200

f : (x)
{
	print(42)
	x
}

add : (x, y)
{
	x + y
}

p : (x, y)
{
	i = x
	while (i <= y)
	{
		print(i)
		i = i + 1
	}	
}


draw_rect : (min_x, min_y, max_x, max_y, color)
{
	y = min_y
	while y < max_y 
	{
		x = min_x
		while x < max_x 
		{
			set_pixel(x % width, y % height, color)
			x = x + 1
		}
		y = y + 1
	}
	
}

sq : (x)
{
	x * x
}

draw_circle : (cx, cy, r, color)
{
	y = cy - r
	while y < cy + r
	{
		x = cx - r
		while x < cx + r 
		{
			if sq(x - cx) + sq(y - cy) < r * r 
			{		
				set_pixel(x % width, y % height, color)
			}
			x = x + 1
		}
		y = y + 1
	}
}


x = 5
y = 5

main : ()
{
	draw_rect(0, 0, width, height, 0x222552f0)
	draw_rect(x, y, x + 50, y + 40, 0xffff00)
	
	draw_circle(width / 2 + x, height / 2 + y, 20, 0xff000000)
	draw_circle(10 + x, 10, 10, 0xff55)
	print(x)
	x = x + 2
	y = y + 2
}

main()